{
  "name": "maintenance-app",
  "version": "25.1.0",
  "description": "DHIS2 Maintenance app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/maintenance-app.git"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "prestart": "d2-manifest package.json manifest.webapp",
    "start": "webpack-dev-server",
    "test": "karma start test/config/karma.config.js --single-run true",
    "test-watch": "karma start test/config/karma.config.js",
    "lint": "eslint src && eslint --env node,mocha --global expect,sinon --rule 'no-unused-expressions: 0' test && scss-lint",
    "__prebuild": "npm test && npm run lint",
    "build": "rm -rf build && webpack && npm run manifest",
    "postbuild": "cp -r src/i18n icon.png node_modules/babel-polyfill/dist/polyfill.min.js node_modules/jquery/dist/jquery.min.js node_modules/ckeditor ./build/",
    "validate": "npm ls",
    "manifest": "d2-manifest package.json build/manifest.webapp"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "3.0.0",
    "classnames": "^2.2.3",
    "coveralls": "2.11.4",
    "css-loader": "^0.23.1",
    "d2": "^25.0.13",
    "d2-manifest": "^1.0.0-2",
    "d2-ui": "^25.1.1",
    "d2-utilizr": "^0.2.9",
    "eslint": "^2.9.0",
    "eslint-config-dhis2": "^2.0.2",
    "eslint-plugin-react": "^4.1.0",
    "fbjs": "^0.2.1",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy": "git+https://github.com/nicolayr/node-http-proxy.git",
    "istanbul": "0.3.18",
    "jquery": "^2.2.3",
    "karma": "0.13.9",
    "karma-babel-preprocessor": "5.2.1",
    "karma-chai": "0.1.0",
    "karma-coverage": "0.5.0",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "1.0.0",
    "karma-sourcemap-loader": "0.3.5",
    "karma-webpack": "^1.8.0",
    "lodash.isfinite": "^3.3.1",
    "lodash.isnumber": "^3.0.3",
    "loglevel": "^1.4.0",
    "material-ui": "^0.15.4",
    "mocha": "2.2.5",
    "moment": "^2.14.1",
    "node-sass": "^3.7.0",
    "phantomjs": "1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "3.0.0",
    "react": "^15.3.1",
    "react-addons-create-fragment": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-linked-state-mixin": "^15.3.1",
    "react-addons-perf": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-color": "^2.2.7",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "rx": "^4.1.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "ckeditor": "^4.5.9",
    "jquery": "^2.2.3"
  },
  "pre-commit": [
    "validate"
  ],
  "manifest.webapp": {
    "name": "Maintenance app",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "Mark Polak"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
